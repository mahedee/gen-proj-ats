                            <li>@Html.ActionLink("Manufacturer", "Index", "Manufacturers")</li>
                            <li>@Html.ActionLink("Model", "Index", "Model")</li>
                            <li>@Html.ActionLink("Supplier", "Index", "Suppliers")</li>
